public class Main
{
	public static void main(String[] args) {
		
	int [] num = {70,50,-100,500,20,90};
    
    int min = num[0];
    
    for(int i = 0; i < num.length; i++){
        if(num[i] < min){
            min = num[i];
        }
    }
    
    System.out.println("the min value int the array is: " + min);
		
	}
}
