class MyEmployee{
    private int id;                        //you dont want that anyone will come and change your data
    private String name;                   //that's the reason we use private access modifier
}                   
                                           //to access private stuff we use getters and setters
public class Main
{
	public static void main(String[] args) {
		MyEmployee harshit = new MyEmployee;
		harshit.id = 1;
		harshit.name = "harshit_programmer";
	}
}


