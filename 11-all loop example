public class Main
{
  public static void main (String[]args)
  {
    System.out.println ("for loop,do while loop and while loop example");



    //using for loop
    
    for (int i = 100; i <= 200; i++)
      {
	System.out.println (i);
      }



    //while loop

    int i = 100;
    while (i <= 200)
      {
	System.out.println (i);
	i++;
      }



    // do while loop 
    
    int i = 100;
    do
      {
	System.out.println (i);
	i++;
      }
    while (i <= 200);
  }
}
