public class Main{
    
    static int logic(int x,int y){
		    int z;
		    
		    if(x>y){
		        z = x + y;
		    } else {
		        z = (x + y) * 5;
		    }
		    
		    return z;
	}
		
		
	public static void main(String[] args) {
		System.out.println("...Introduction to java methods(functions)...");
		
		int a = 8;
		int c;
		int b = 4;
		c = logic(a,b);
		System.out.println(c);
		
		
	}
}
