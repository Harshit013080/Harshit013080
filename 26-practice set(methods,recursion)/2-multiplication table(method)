import java.util.Scanner;

public class Main
{
    
    static void multi(int n){
        for(int i = 1; i <= 10; i++) {
            System.out.printf("%d * %d = %d \n",n,i,i*n);
        }
    }
    
	public static void main(String[] args) {
	    
	    Scanner sc = new Scanner(System.in);
	    
		System.out.print("Enter the number of which you want to print the table: ");
		int num = sc.nextInt();
		
		multi(num);
		
	}
}
