public class Main
{
  public static void main (String[]args)
  {
    System.out.println ("Calculating the sum of the multiplication answer");

    int sum = 0;

    int num = 8;
    for (int i = 1; i <= 10; i++)
      {
	sum = sum + (num * i);
      }
    System.out.print ("The sum is: ");
    System.out.println (sum);
  }
}
